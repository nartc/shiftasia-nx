{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/cart-publishable/src",
  "prefix": "shiftasia-nx",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/cart-publishable"],
      "options": {
        "project": "libs/cart-publishable/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/cart-publishable/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/cart-publishable/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/cart-publishable"],
      "options": {
        "jestConfig": "libs/cart-publishable/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/cart-publishable/**/*.ts",
          "libs/cart-publishable/**/*.html"
        ]
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm publish",
        "cwd": "dist/libs/cart-publishable"
      }
    }
  },
  "tags": []
}
